{
  "manifest_version": 3,
  "name": "Resources Application",
  "version": "1.0",
  "description": "Espone i dati di localstorage e cookie",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "css": ["css/popup.css", "css/button.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
